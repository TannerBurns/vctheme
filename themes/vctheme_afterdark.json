{
    "name": "VCTheme Night.v1",
    "type": "dark",
    "colors": {
        "editorLineNumber.activeForeground": "#006eff",

        "sideBar.background": "#132639",
        "panel.background": "#132639",
        "editorGroupHeader.tabsBackground": "#132639",
        "tab.inactiveBackground": "#132639",

        "activityBar.background": "#0d1a26",
        "titleBar.activeBackground": "#0d1a26",
        "editor.background": "#0d1a26",

        "editor.foreground":  "#FFFFFF"
    },
    "tokenColors": [
        {
            "name": "Source",
            "scope": [
                "source",
                "text.html",
                "punctuation.definition.string.begin.html",
                "punctuation.definition.string.end.html",
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Red_Types_Italic",
            "scope": [
                "keyword.function",
                "keyword.type"
            ],
            "settings": {
                "foreground": "#ff8080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Red_Types_Non_Italic",
            "scope": [
                "constant.language",
                "variable.language.this",
                "variable.language.special.self",
                "string.json",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#ff8080"
            }
        },
        {
            "name": "Purple_Types_Italic",
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#ccb3ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Purple_Types_Non_Italic",
            "scope": [
                "keyword.operator",
                "constant.other",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#ccb3ff"
            }
        },
        {
            "name": "Blue_Types_Non_Italic",
            "scope": [
                "entity.name.function",
                "meta.function-call.generic",
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#b3d1ff"
            }
        },
        {
            "name": "Dark_Blue_Types_Non_Italic",
            "scope": [
                "support.function",
                "keyword.struct",
                "keyword.interface",
                "entity.name.function",
                "entity.name.tag",
                "text.log",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#80b3ff"
            }
        },
        {
            "name": "Green_Types_Non_Italic",
            "scope": [
                "string.quoted",
                "string.template",
                "log.string",
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "foreground": "#b3ffb3"
            }
        },
        {
            "name": "Grey_Types_Non_Italic",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#999999"
            }
        },
        {
            "name": "Orange_Types_Non_Italic",
            "scope": [
                "variable.other.assignment",
                "entity.other.attribute-name.html",
                "support.variable",
                "entity.other.attribute-name.id.css",
                "meta.function-call.arguments",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#ffd9b3"
            }
        }

    ]
}