{
    "name": "VCTheme Day.v1",
    "type": "dark",
    "colors": {
        "editorLineNumber.activeForeground": "#006eff",

        "sideBar.background": "#595959",
        "panel.background": "#4d4d4d",
        "editorGroupHeader.tabsBackground": "#404040",
        "tab.inactiveBackground": "#262626",

        "activityBar.background": "#404040",
        "titleBar.activeBackground": "#595959",

        "editor.background": "#4d4d4d",
        "editor.foreground":  "#ffffff"
    },
    "tokenColors": [
        {
            "name": "Source",
            "scope": [
                "source",
                "text.html",
                "punctuation.definition.string.begin.html",
                "punctuation.definition.string.end.html",
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Red_Types_Italic",
            "scope": [
                "keyword.function",
                "keyword.type",
                "variable.language.this",
                "variable.language.special.self"
            ],
            "settings": {
                "foreground": "#ff8080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Red_Types_Non_Italic",
            "scope": [
                "constant.language",
                "string.json",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#558000"
            }
        },
        {
            "name": "Purple_Types_Italic",
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#ccb3ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Purple_Types_Non_Italic",
            "scope": [
                "keyword.operator",
                "constant.other",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#ccb3ff"
            }
        },
        {
            "name": "Orange_Types_Non_Italic",
            "scope": [
                "entity.name.function",
                "meta.function-call.generic",
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#e68a00"
            }
        },
        {
            "name": "Dark_Blue_Types_Non_Italic",
            "scope": [
                "support.function",
                "keyword.struct",
                "keyword.interface",
                "entity.name.function",
                "entity.name.tag",
                "text.log",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#80b3ff"
            }
        },
        {
            "name": "Green_Types_Non_Italic",
            "scope": [
                "string.quoted",
                "string.template",
                "log.string",
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "foreground": "#b3ffb3"
            }
        },
        {
            "name": "Grey_Types_Non_Italic",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#999999"
            }
        },
        {
            "name": "Orange_Types_Non_Italic",
            "scope": [
                "variable.other.assignment",
                "entity.other.attribute-name.html",
                "support.variable",
                "entity.other.attribute-name.id.css",
                "meta.function-call.arguments",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#ffd9b3"
            }
        }

    ]
}